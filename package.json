          "group": "1_actions"
        },
        {
          "command": "cursor-toys.shareHooksViaGist",
          "when": "view == cursor-toys.userHooks && viewItem == hooksFile",
          "group": "1_actions"
        },
        {
          "command": "cursor-toys.revealHooks",
          "when": "view == cursor-toys.userHooks && viewItem == hooksFile",
          "group": "1_actions"
        },
        {
          "command": "cursor-toys.deleteHooks",
