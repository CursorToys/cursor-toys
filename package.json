      ],
      "cursor-toys": [
        {
          "command": "cursor-toys.generate-command",
          "when": "resourcePath =~ /\\.(cursor|claude|vscode|ai)\\/commands\\// && !explorerResourceIsFolder"
        },
        {
          "command": "cursor-toys.shareAsCursorToysCommand",
          "when": "resourcePath =~ /\\.(cursor|claude|vscode|ai)\\/commands\\// && !explorerResourceIsFolder"
        },
        {
          "command": "cursor-toys.save-as-user-command",
          "when": "resourcePath =~ /\\.(cursor|claude|vscode|ai)\\/commands\\// && !explorerResourceIsFolder"
        },
        {
          "command": "cursor-toys.shareAsCursorToysCommandFolder",
          "when": "explorerResourceIsFolder && resourcePath =~ /[\\\\/]\\.(cursor|claude|vscode|ai)[\\\\/]commands($|[\\\\/])/"
        },
        {
          "command": "cursor-toys.generate-rule",
          "when": "resourcePath =~ /\\.(cursor|vscode|ai)\\/rules\\// && !explorerResourceIsFolder"
        },
        {
          "command": "cursor-toys.shareAsCursorToysRule",
          "when": "resourcePath =~ /\\.(cursor|vscode|ai)\\/rules\\// && !explorerResourceIsFolder"
        },
        {
          "command": "cursor-toys.shareAsCursorToysRuleFolder",
          "when": "explorerResourceIsFolder && resourcePath =~ /[\\\\/]\\.(cursor|vscode|ai)[\\\\/]rules($|[\\\\/])/"
        },
        {
          "command": "cursor-toys.generate-prompt",
          "when": "resourcePath =~ /\\.(cursor|vscode|ai)\\/prompts\\// && !explorerResourceIsFolder"
        },
        {
          "command": "cursor-toys.shareAsCursorToysPrompt",
          "when": "resourcePath =~ /\\.(cursor|vscode|ai)\\/prompts\\// && !explorerResourceIsFolder"
        },
        {
          "command": "cursor-toys.save-as-user-prompt",
          "when": "resourcePath =~ /\\.(cursor|vscode|ai)\\/prompts\\// && !explorerResourceIsFolder"
        },
        {
          "command": "cursor-toys.shareAsCursorToysPromptFolder",
          "when": "explorerResourceIsFolder && resourcePath =~ /[\\\\/]\\.(cursor|vscode|ai)[\\\\/]prompts($|[\\\\/])/"
        },
        {
          "command": "cursor-toys.shareAsCursorToysNotepad",
          "when": "resourcePath =~ /\\.(cursor|vscode|ai)\\/notepads\\// && !explorerResourceIsFolder"
        },
        {
          "command": "cursor-toys.shareAsCursorToysNotepadFolder",
          "when": "explorerResourceIsFolder && resourcePath =~ /[\\\\/]\\.(cursor|vscode|ai)[\\\\/]notepads($|[\\\\/])/"
        },
        {
          "command": "cursor-toys.shareAsCursorToysPlan",
          "when": "resourcePath =~ /\\.(cursor|vscode|ai)\\/plans\\// && !explorerResourceIsFolder && resourceFilename =~ /\\.plan\\.md$/"
        },
        {
          "command": "cursor-toys.shareAsCursorToysPlanFolder",
          "when": "explorerResourceIsFolder && resourcePath =~ /[\\\\/]\\.(cursor|vscode|ai)[\\\\/]plans($|[\\\\/])/"
        },
        {
          "command": "cursor-toys.generate-skill",
          "when": "resourceFilename == SKILL.md"
        },
        {
          "command": "cursor-toys.shareAsCursorToysSkill",
          "when": "resourceFilename == SKILL.md"
        },
        {
          "command": "cursor-toys.createSkill",
          "when": "explorerResourceIsFolder && resourcePath =~ /[\\\\/]\\.(cursor|claude|vscode|ai)[\\\\/]skills($|[\\\\/])/",
          "group": "navigation"
        },
        {
          "command": "cursor-toys.shareAsCursorToysSkillFolder",
          "when": "explorerResourceIsFolder && resourcePath =~ /[\\\\/]\\.(cursor|claude|vscode|ai)[\\\\/]skills($|[\\\\/])/"
        },
        {
          "command": "cursor-toys.save-as-user-skill",
          "when": "explorerResourceIsFolder && resourcePath =~ /[\\\\/]\\.(cursor|claude|vscode|ai)[\\\\/]skills\\/[^\\\\/]+$/"
        },
        {
          "command": "cursor-toys.save-as-user-skill",
          "when": "resourceFilename == SKILL.md && resourcePath =~ /[\\\\/]\\.(cursor|claude|vscode|ai)[\\\\/]skills\\/[^\\\\/]+\\/SKILL\\.md$/"
        },
        {
          "command": "cursor-toys.shareAsCursorToysHttp",
          "when": "resourcePath =~ /[\\\\/]\\.(cursor|vscode|ai)[\\\\/]http[\\\\/].*\\.(req|request)$/ && !explorerResourceIsFolder"
        },
        {
          "command": "cursor-toys.shareAsCursorToysEnv",
          "when": "resourcePath =~ /[\\\\/]\\.(cursor|vscode|ai)[\\\\/]http[\\\\/](environments|\\.environments|__environments__|_env)[\\\\/]\\.env/ && !explorerResourceIsFolder"
        },
        {
          "command": "cursor-toys.shareAsCursorToysHttpWithPath",
          "when": "resourcePath =~ /[\\\\/]\\.(cursor|vscode|ai)[\\\\/]http[\\\\/].*\\.(req|request)$/ && !explorerResourceIsFolder"
        },
        {
          "command": "cursor-toys.shareAsCursorToysEnvWithPath",
          "when": "resourcePath =~ /[\\\\/]\\.(cursor|vscode|ai)[\\\\/]http[\\\\/](environments|\\.environments|__environments__|_env)[\\\\/]\\.env/ && !explorerResourceIsFolder"
        },
        {
          "command": "cursor-toys.shareAsCursorToysHttpFolder",
          "when": "explorerResourceIsFolder && resourcePath =~ /[\\\\/]\\.(cursor|vscode|ai)[\\\\/]http($|[\\\\/])/"
        },
        {
          "command": "cursor-toys.shareAsCursorToysHttpFolderWithEnv",
          "when": "explorerResourceIsFolder && resourcePath =~ /[\\\\/]\\.(cursor|vscode|ai)[\\\\/]http($|[\\\\/])/"
        },
        {
          "command": "cursor-toys.shareAsCursorToysEnvFolder",
          "when": "explorerResourceIsFolder && resourcePath =~ /[\\\\/]\\.(cursor|vscode|ai)[\\\\/]http[\\\\/](environments|\\.environments|__environments__|_env)($|[\\\\/])/"
        },
        {
          "command": "cursor-toys.shareAsCursorToysProject",
          "when": "explorerResourceIsFolder && resourcePath =~ /[\\\\/]\\.(cursor|vscode|ai)$/"
        },
        {
          "command": "cursor-toys.shareHooks",
          "when": "resourceFilename == hooks.json"
        },
        {
          "command": "cursor-toys.shareHooksViaGist",
          "when": "resourceFilename == hooks.json"
        },
        {
          "command": "cursor-toys.initializeEnvironments",
          "when": "explorerResourceIsFolder && resourcePath =~ /[\\\\/]\\.(cursor|vscode|ai)[\\\\/]http($|[\\\\/])/"
        },
        {
          "command": "cursor-toys.shareViaGist",
          "when": "(resourceExtname =~ /\\.(md|mdc|req|request)$/ || resourceFilename =~ /^\\.env/) && !explorerResourceIsFolder"
        },
        {
          "command": "cursor-toys.shareFolderViaGist",
          "when": "explorerResourceIsFolder && (resourcePath =~ /[\\\\/]\\.(cursor|claude|vscode|ai)[\\\\/](commands|rules|prompts|notepads|http)($|[\\\\/])/ || resourcePath =~ /[\\\\/]\\.(cursor|vscode|ai)$/)"
        }
      ],
      "editor/context": [
        {
          "submenu": "cursor-toys-selection",
          "when": "editorHasSelection",
          "group": "navigation"
        },
        {
          "command": "cursor-toys.minifyFile",
          "when": "resourceExtname =~ /\\.(json|html|htm|xml|css|svg|js|jsx|ts|tsx)$/",
          "group": "cursorToys@1"
        }
      ],
      "cursor-toys-selection": [
        {
          "command": "cursor-toys.sendSelectionToChat",
          "when": "editorHasSelection"
        },
        {
          "command": "cursor-toys.copySelectionAsPrompt",
          "when": "editorHasSelection"
        }
      ],
      "editor/title": [
        {
          "command": "cursor-toys.minifyFile",
          "when": "resourceExtname =~ /\\.(json|html|htm|xml|css|svg|js|jsx|ts|tsx)$/",
          "group": "navigation"
        }
      ],
      "commandPalette": [
        {
          "command": "cursor-toys.sendSelectionToChat",
          "when": "editorHasSelection"
        },
        {
          "command": "cursor-toys.minifyFile",
          "when": "editorIsOpen"
        },
        {
          "command": "cursor-toys.trimClipboard",
          "when": "true"
        },
        {
          "command": "cursor-toys.trimClipboardWithPrompt",
          "when": "true"
        },
        {
          "command": "cursor-toys.refineSelectionWithAI",
          "when": "editorHasSelection"
        },
        {
          "command": "cursor-toys.refineClipboardWithAI",
          "when": "true"
        },
        {
          "command": "cursor-toys.configureAIProvider",
          "when": "true"
        },
        {
          "command": "cursor-toys.removeAIProviderKey",
          "when": "true"
        }
      ]
    },
    "configuration": {
      "title": "Cursor Toys",
      "properties": {
        "cursorToys.linkType": {
          "type": "string",
          "enum": [
            "deeplink",
            "web",
            "custom"
          ],
          "enumDescriptions": [
            "Use cursor:// protocol (deeplink)",
            "Use web URL https://cursor.com/link/",
            "Use a custom base URL (configure in cursorToys.customBaseUrl)"
          ],
          "default": "deeplink",
          "description": "Type of link to generate"
        },
        "cursorToys.customBaseUrl": {
          "type": "string",
          "default": "",
          "description": "Custom base URL for deeplinks (only used when linkType is 'custom'). Example: https://example.com/link/ or custom://protocol/"
        },
        "cursorToys.allowedExtensions": {
          "type": "array",
          "items": {
            "type": "string"
          },
          "default": [
            "md",
            "mdc"
          ],
          "description": "Allowed file extensions (without the dot, e.g., md, mdc, txt, json). For rules, .mdc is recommended to support metadata."
        },
        "cursorToys.baseFolder": {
          "type": "string",
          "default": "cursor",
          "pattern": "^[a-zA-Z0-9_-]+$",
          "description": "Base folder name for storing commands, rules, prompts and HTTP files (e.g., 'cursor', 'vscode', 'ai'). Note: Rules and prompts are Cursor-specific features."
        },
        "cursorToys.commandsFolder": {
          "type": "string",
          "enum": [
            "cursor",
            "claude"
          ],
          "enumDescriptions": [
            "Use .cursor/commands folder (default)",
            "Use .claude/commands folder"
          ],
          "default": "cursor",
          "description": "Folder to save commands: 'cursor' for .cursor/commands or 'claude' for .claude/commands. Only the commands folder is configurable; rules and prompts always use .cursor"
        },
        "cursorToys.personalCommandsView": {
          "type": "string",
          "enum": [
            "both",
            "cursor",
            "claude"
          ],
          "enumDescriptions": [
            "Show commands from both .cursor and .claude folders",
            "Show commands from .cursor folder only",
            "Show commands from .claude folder only"
          ],
          "default": "both",
          "description": "Which command folders to show in Personal Commands view: 'both' for .cursor and .claude, 'cursor' for .cursor only, or 'claude' for .claude only"
        },
        "cursorToys.httpRequestTimeout": {
          "type": "number",
          "default": 10,
          "description": "Timeout in seconds for HTTP requests (default: 10 seconds)"
        },
        "cursorToys.httpRequestSaveFile": {
          "type": "boolean",
          "default": false,
          "description": "Save HTTP response to file. If false, only shows preview without saving."
        },
        "cursorToys.httpDefaultEnvironment": {
          "type": "string",
          "default": "dev",
          "description": "Default HTTP environment name (used for reference in status bar)"
        },
        "cursorToys.environmentsFolder": {
          "type": "string",
          "enum": [
            ".environments",
            "environments",
            "__environments__",
            "_env"
          ],
          "enumDescriptions": [
            "Hidden folder: .environments (recommended)",
            "Standard folder: environments",
            "Special folder: __environments__",
            "Short hidden folder: _env"
          ],
          "default": ".environments",
          "description": "Name of the folder to store HTTP environment files inside the http/ directory"
        },
        "cursorToys.minify.preserveComments": {
          "type": "boolean",
          "default": false,
          "description": "Preserve comments when minifying code (currently not implemented)"
        },
        "cursorToys.minify.outputSuffix": {
          "type": "string",
          "default": ".min",
          "description": "Suffix for minified files (e.g., '.min' creates 'file.min.js')"
        },
        "cursorToys.gistDefaultVisibility": {
          "type": "string",
          "enum": [
            "public",
            "private",
            "ask"
          ],
          "enumDescriptions": [
            "Always create public gists",
            "Always create private gists",
            "Ask each time before creating a gist"
          ],
          "default": "ask",
          "description": "Default visibility when creating GitHub Gists"
        },
        "cursorToys.recommendationsEnabled": {
          "type": "boolean",
          "default": true,
          "description": "Enable CursorToys recommendations based on project context"
        },
        "cursorToys.recommendationsCheckOnStartup": {
          "type": "boolean",
          "default": true,
          "description": "Check for recommendations when opening a workspace"
        },
        "cursorToys.recommendationsSuggestInterval": {
          "type": "number",
          "default": 7,
          "minimum": 1,
          "description": "Days between recommendation suggestions (default: 7)"
        },
        "cursorToys.recommendationsIndexUrl": {
          "type": "string",
          "default": "https://raw.githubusercontent.com/CursorToys/marketplace/refs/heads/main/recommendations-index.json",
          "description": "URL to remote recommendations index (raw file URL from GitHub or Gist). Default: CursorToys official marketplace (contributions via PR)"
        },
        "cursorToys.recommendationsIndexGistId": {
          "type": "string",
          "default": "",
          "description": "Gist ID containing recommendations index (e.g., 'username/gistid' or just 'gistid'). Alternative to recommendationsIndexUrl for private/custom indexes."
        },
        "cursorToys.aiProvider": {
          "type": "string",
          "enum": [
            "gemini"
          ],
          "default": "gemini",
          "description": "AI provider to use for text refinement"
        },
        "cursorToys.aiModel": {
          "type": "string",
          "default": "",
          "description": "AI model to use (optional, e.g., 'gemini-2.5-flash', 'gemini-2.0-flash', 'gemini-2.5-pro'). If empty, uses 'gemini-2.5-flash' as default."
        },
        "cursorToys.aiRefinePrompt": {
          "type": "string",
          "default": "You must return ONLY the refined text, nothing else. Do not add introductions like \"Here's the refined version\" or \"Okay, here is\". Do not add markdown separators (---). Do not add explanations or notes. Just return the improved text directly.\n\nFix typos, improve clarity, and enhance the flow of the following text:",
          "description": "Prompt template for text refinement"
        },
        "cursorToys.aiRequestTimeout": {
          "type": "number",
          "default": 30,
          "description": "Timeout in seconds for AI requests"
        }
      }
    },
    "languages": [
      {
        "id": "http-request",
        "aliases": [
          "HTTP Request",
          "http-request"
        ],
        "extensions": [
          ".req",
          ".request"
        ]
      },
      {
        "id": "http-response",
        "aliases": [
          "HTTP Response",
          "http-response"
        ],
        "extensions": [
          ".res",
          ".response"
        ]
      }
    ],
    "grammars": [
      {
        "language": "http-request",
        "scopeName": "text.http-request",
        "path": "./syntaxes/http-request.tmLanguage.json"
      },
      {
        "language": "http-response",
        "scopeName": "text.http-response",
        "path": "./syntaxes/http-response.tmLanguage.json"
      }
    ],
    "keybindings": [
      {
        "command": "cursor-toys.import",
        "key": "ctrl+shift+i",
        "mac": "cmd+shift+i"
      },
      {
        "command": "cursor-toys.refineSelectionWithAI",
        "key": "ctrl+shift+r",
        "mac": "cmd+shift+r"
      },
      {
        "command": "cursor-toys.refineClipboardWithAI",
        "key": "ctrl+alt+shift+r",
        "mac": "cmd+alt+shift+r"
      }
    ],
    "uriHandlers": [
      {
        "id": "cursor-toys",
        "scheme": "cursor",
        "host": "godrix.cursor-toys"
      },
      {
        "id": "vscode-cursor-toys",
        "scheme": "vscode",
        "host": "godrix.cursor-toys"
      }
    ]
  },
  "scripts": {
    "vscode:prepublish": "npm run compile",
    "compile": "tsc -p ./",
    "watch": "tsc -watch -p ./",
    "package": "vsce package",
    "publish": "vsce publish"
  },
  "dependencies": {
    "@google/genai": "^1.38.0"
  },
  "devDependencies": {
    "@types/vscode": "^1.74.0",
    "@types/node": "16.x",
    "typescript": "^4.9.4"
  }
